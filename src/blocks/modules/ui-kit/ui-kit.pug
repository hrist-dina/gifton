                label.contacts-form__label
                    textarea.contacts-form__textarea(name="message", placeholder="Сообщение")




    section.registration
        .container
            .registration__inner
                .registration-left
                    .contacts-form__description Оформление:
                    img(src='img/registration/registration.png')
                .registration-right
                    .registration-form
                    form.registration-form(action="", method="POST")
                        label.registration-form__label
                            input.registration-form__input-text(name="name",type="text" placeholder="Имя Фамилия")
                        label.registration-form__label
                            input.registration-form__input-text(name="email",type="text", placeholder="Email")
                        label.registration-form__label
                            input.registration-form__input-text(name="name",type="text" placeholder="Телефон")
                        label.registration-form__label
                            input.registration-form__input-text.registration-form__input-text_active(name="name",type="text" placeholder="Advantage Media")
                        label.registration-form__label
                            input.registration-form__input-text(name="name",type="text" placeholder="Сайт")
                        label.registration-form__label
                            textarea.registration-form__textarea(name="message", placeholder="Комментарий")
